{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "src/**",
      "src/components/CustomTreeView.vue"
    ]
  },
  "rules": {
    "trailing-comma": false,
    "no-console": false,
    "quotemark": [
      false,
      "single"
    ],
    "semicolon": [
      false,
      "never"
    ],
    "indent": [
      true,
      "spaces",
      4
    ],
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false
  }
}